    <import addon="script.module.t1mlib" version="1.0.18"/>
    <import addon="script.module.metahandler" version="2.9.1"/>
  </requires>
  <extension point="xbmc.python.pluginsource"
            library="default.py">
        <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
